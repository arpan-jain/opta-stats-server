{
  "extends": "airbnb/legacy", //ES5 checker
  "rules": {
    "func-names": ["error", "never"],
    "es6" : 1,
    "no-param-reassign": 0,  // because for node/loopback we need to modify the original construvtor in controllers.
    "one-var-declaration-per-line" : 0,
    "one-var" : 0,
    "object-curly-spacing" :0,
    "vars-on-top":0,
    "space-before-function-paren":0
  }
}
